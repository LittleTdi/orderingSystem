.my-service-center{
  background:#F8F8F8;
  height:100vh;
}
.my-service-center .time{
  color:rgba(17, 26, 44, 0.3);
  padding:40rpx 0;
}
.my-service-center .dialog .content{
  padding:0 32rpx;
}
.my-service-center .dialog .content>view:last-child{
  max-width: 494.56rpx;
  padding:20rpx 24rpx 20rpx 22rpx;
  border-radius: 0 32rpx 32rpx 32rpx;
  margin-left:24rpx;
}
.my-service-center .dialog .content>view:first-child{
  width:72rpx;
  height:72rpx;
  background:#4CAF50;
  border-radius: 50%;
  font-size: 10px;
  padding:8rpx 16rpx;
}
.my-service-center .dialog .content>view:first-child>view{
  margin-top:10rpx;
}
.my-service-center .dialog .content>view:first-child>view:first-child{
  margin:0;
}