<view class="page">
  <view class="d-flex my-info">
    <view class="avatar"><open-data type="userAvatarUrl"></open-data></view>
    <view><open-data type="userNickName"></open-data></view>
    <view></view>
  </view>
  <view class="report">
    <view class="d-flex flex-between border-b" bindtap="toSelfCheck">
      <view class="d-flex">
        <image src="../../images/m1.png"></image>
        <view>我的健康自查上报</view>
      </view>
      <image src="../../images/m4.png"></image>
    </view>
    <view class="d-flex flex-between border-b" bindtap="toclue">
      <view class="d-flex">
        <image src="../../images/m2.png"></image>
        <view>我的疫情线索上报</view>
      </view>
      <image src="../../images/m4.png"></image>
    </view>
    <view class="d-flex flex-between border-b" bindtap="toSubcribe">
      <view class="d-flex">
        <image src="../../images/m3.png"></image>
        <view>我的预约</view>
      </view>
      <image src="../../images/m4.png"></image>
    </view>
  </view>
  <button>退出登录</button>
</view>
