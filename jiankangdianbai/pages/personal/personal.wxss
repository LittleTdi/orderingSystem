.my-info{
  padding:20px 15px;
  background:#fff;
}
.avatar{
  height:50px;
  width:50px;
  margin-right:10px;
  border-radius:25px;
  overflow: hidden;
}
.page{
  background:#f5f5f5;
  height:100vh;
  width:100vw;
}
.report{
  background:#fff;
  margin-top:15px;
}
.report image{
  height:25px;
  width:25px;
}
.report .border-b{
  padding:0 10px;
  height:44px;
}
.report .border-b>image{
  width:12px;
  height:17px;
}
.page>button{
  width:95%;
  background:#fff;
  font-weight: 500;
  margin-top:20px;
  padding:3px;
}
.report .border-b>view>image{
  margin-right:5px;
}